{
  "name": "triopsi-block-visibility",
  "version": "2.0.0",
  "description": "Example block scaffolded with Create Block tool.",
  "author": "The WordPress Contributors",
  "license": "GPL-2.0-or-later",
  "main": "build/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "build:editor": "vite build --config vite.config.js",
    "build:styles": "vite build --config vite.frontend.config.js",
    "build": "npm run clean && npm run build:editor && npm run build:styles",
    "dev:editor": "vite build --watch --config vite.config.js",
    "dev:styles": "vite build --watch --config vite.frontend.config.js",
    "dev": "npm run dev:editor & npm run dev:styles",
    "lint:js": "wp-scripts lint-js",
    "lint:css": "wp-scripts lint-style",
    "format": "wp-scripts format",
    "plugin-zip": "wp-scripts plugin-zip",
    "packages-update": "wp-scripts packages-update"
  },
  "devDependencies": {
    "@wordpress/scripts": "^31.4.0",
    "sass": "^1.83.0",
    "vite": "^5.4.11"
  }
}
